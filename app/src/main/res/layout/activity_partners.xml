                            android:textColor="@color/black"
                            android:textSize="@dimen/_11sdp" />
                    </RelativeLayout>

                    <androidx.cardview.widget.CardView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_6sdp"
                        android:layout_marginEnd="@dimen/_6sdp"
                        app:cardCornerRadius="@dimen/_6sdp"
                        app:cardElevation="@dimen/_1sdp">

                        <RelativeLayout
                            android:id="@+id/rl_friends"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="@dimen/_6sdp"
                            android:background="@drawable/ic_rectangle_pat1">

                            <androidx.recyclerview.widget.RecyclerView
                                android:id="@+id/rcy_friends"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content" />

                        </RelativeLayout>
                    </androidx.cardview.widget.CardView>

                    <RelativeLayout
                        android:id="@+id/rl_Government_2"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_6sdp"
                        android:layout_marginTop="@dimen/_10sdp">

                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:background="@color/white"
                            android:fontFamily="@font/poppins_medium"
                            android:gravity="center|left"
                            android:text="@string/government_partners"
                            android:textColor="@color/black"
                            android:textSize="@dimen/_11sdp"></TextView>
                    </RelativeLayout>

                    <androidx.cardview.widget.CardView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_6sdp"
                        android:layout_marginEnd="@dimen/_6sdp"
                        app:cardCornerRadius="@dimen/_6sdp"
                        app:cardElevation="@dimen/_1sdp">

                        <RelativeLayout
                            android:id="@+id/rl_Government_Partners"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_marginEnd="@dimen/_6sdp">

                            <androidx.recyclerview.widget.RecyclerView
                                android:id="@+id/rcy_Government_Partners"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content" />

                        </RelativeLayout>
                    </androidx.cardview.widget.CardView>


                </LinearLayout>


            </androidx.core.widget.NestedScrollView>

            <RelativeLayout
                android:id="@+id/ShowREl"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="@color/white"></RelativeLayout>

        </RelativeLayout>
    </LinearLayout>
</layout>